RM = rm -f

CC = gcc

distr :     CFLAGS =                    \
            -I/usr/openwin/include      \
            -ffloat-store               \
            -fsigned-char               \
            -fwritable-strings          \
            -O                          \
            -DUNIX                      \
            -DV3_X11                    \
            -DVARKON                    \
            -DWRK_STAT

COMPILE = $(CC) -c $(CFLAGS)

include Makefile.PM

